<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.flowchart"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="8"
        android:targetSdkVersion="19" />

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.flowchart.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.flowchart.Start_Activity"
            android:label="@string/title_activity_start_" >
        </activity>
        <activity
            android:name="com.example.flowchart.Images_Activity"
            android:label="@string/title_activity_images_" >
        </activity>
        <activity
            android:name="com.example.flowchart.Saved_Activity"
            android:label="@string/title_activity_saved_" >
        </activity>
        <activity
            android:name="com.example.flowchart.About"
            android:label="@string/title_activity_about" >
        </activity>
        <activity
            android:name="com.example.flowchart.Help"
            android:label="@string/title_activity_help" >
        </activity>
    </application>

</manifest>
